    "@solana/web3.js": "^1.29.2",
    "@terra-money/terra.js": "3.0.9",
    "@terra-money/wallet-provider": "^3.8.0",
    "crypto-js": "^4.1.1",
    "google-libphonenumber": "^3.2.22",
    "isomorphic-fetch": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "polished": "^4.1.1",
